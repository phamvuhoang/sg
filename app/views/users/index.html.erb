<h1>Users</h1>

<table class="table table-stripped">
<thead class="thead thead-default">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Created</th>
  </tr>
</thead>
<% @users.each do |user| %>
<tr>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.created_at %></td>
</tr>
<% end %>
  
</table>
